{
  "name": "hackerweb",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "current-version": "node -e 'console.log(require(\"./app.json\").expo.version)'",
    "build": "expo build:ios --release-channel staging-`npm run current-version --silent`",
    "build-prod": "expo build:ios --release-channel production-`npm run current-version --silent`",
    "latest-staging": "expo publish:history -p ios --count 1 -r -c staging-`npm run current-version --silent`",
    "latest-publish-id": "node -e \"console.log($(npm run latest-staging --silent).queryResult[0].publicationId)\"",
    "publish": "expo publish --release-channel staging-`npm run current-version --silent`",
    "promote-prod": "expo publish:set --publish-id `npm run latest-publish-id --silent` --release-channel production-`npm run current-version --silent`",
    "viz": "npx react-native-bundle-visualizer --expo managed"
  },
  "dependencies": {
    "@expo/metro-config": "~0.1.33",
    "@react-native-async-storage/async-storage": "^1.13.0",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/segmented-control": "2.2.1",
    "@react-navigation/native": "~5.7.4",
    "@react-navigation/stack": "~5.9.1",
    "array-move": "~3.0.1",
    "buffer": "~5.6.0",
    "date-fns": "~2.16.1",
    "entities": "~2.0.3",
    "expo": "^41.0.0",
    "expo-blur": "~9.0.3",
    "expo-constants": "~10.1.3",
    "expo-device": "~3.2.0",
    "expo-haptics": "~10.0.0",
    "expo-linear-gradient": "~9.1.0",
    "expo-linking": "~2.2.3",
    "expo-mail-composer": "~10.1.2",
    "expo-splash-screen": "~0.10.2",
    "expo-status-bar": "~1.0.4",
    "expo-store-review": "~4.0.2",
    "expo-updates": "~0.5.4",
    "expo-web-browser": "~9.1.0",
    "htmlparser2": "~5.0.1",
    "ky": "~0.24.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-modalize": "^2.0.6",
    "react-native-reanimated": "~2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-svg": "12.1.0",
    "react-native-url-polyfill": "^1.2.0",
    "react-native-web": "~0.13.12",
    "react-native-webview": "11.2.3",
    "stream": "0.0.2",
    "url-regex-safe": "~1.0.2",
    "zustand": "~3.1.3"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "react-native-svg-transformer": "~0.14.3"
  }
}
