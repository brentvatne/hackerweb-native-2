{
  "expo": {
    "name": "HackerWebby",
    "slug": "hackerweb-native-2",
    "version": "2.0.1",
    "description": "A read-only Hacker News client.",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 1000
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "appStoreUrl": "https://apps.apple.com/app/id1084209377",
      "supportsTablet": false,
      "bundleIdentifier": "xyz.bront.hackerweb",
      "buildNumber": "10"
    },
    "platforms": [
      "ios"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "userInterfaceStyle": "automatic",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
